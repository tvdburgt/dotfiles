# Start music client
"urxvt -e ncmpcpp"
    m:0x0 + c:179
    XF86Tools

# Toggle music
"mpc toggle"
    m:0x10 + c:172
    XF86AudioPlay

# Next song
"mpc next"
    m:0x4 + c:86
    Control + KP_Add

# Previous song
"mpc prev"
    m:0x4 + c:82
    Control + KP_Subtract

# Toggle mute
"amixer set Master toggle"
    m:0x10 + c:121
    XF86AudioMute

# Increase volume
"amixer set Master playback 1+"
    m:0x0 + c:123
    XF86AudioRaiseVolume

# Decrease volume
"amixer set Master playback 1-"
    m:0x0 + c:122
    XF86AudioLowerVolume

# Open browser
"$BROWSER"
    m:0x0 + c:180
    XF86HomePage
